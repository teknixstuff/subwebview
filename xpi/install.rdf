<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>subwebview@teknixstuff.com</em:id>
    <em:version>1.0.3</em:version>
    <em:type>2</em:type>
    <em:unpack>true</em:unpack>
    <em:iconURL>chrome://subwebview/content/logo.png</em:iconURL>
    <em:optionsURL>chrome://subwebview/content/settings.xul</em:optionsURL>

    <!-- Pale Moon -->
    <em:targetApplication>
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>27.1.0</em:minVersion>
        <em:maxVersion>33.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Basilisk, Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>39.0.0</em:minVersion>
        <em:maxVersion>56.*</em:maxVersion>
        <em:basilisk>true</em:basilisk>
      </Description>
    </em:targetApplication>

    <!-- front-end metadata -->
    <em:name>SubWebView</em:name>
    <em:description>View web pages using WebView inside another browser</em:description>
    <em:creator>Tech Stuff</em:creator>

  </Description>
</RDF>
